<UserControl
    x:Class="string_matching_algorithm.Views.AlgorithmView"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:string_matching_algorithm.Views"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    d:DesignHeight="450"
    d:DesignWidth="800"
    Background="#ffff"
    mc:Ignorable="d">
    <StackPanel>
        <Button Command="{Binding NavigateHomeCommand}" Content="Back" />
        <Label Content="Algorithm" />
        <Button Command="{Binding NavigateNaiveCommand}" Content="Naive Algorithm" />
        <Button Command="{Binding NavigateKMPCommand}" Content="KMP Algorithm" />
        <Button Command="{Binding NavigateRabinKarpCommand}" Content="Rabin Karp Algorithm" />
        <Button Command="{Binding NavigateBoyerMooreCommand}" Content="Boyer Moore Algorithm" />
    </StackPanel>
</UserControl>
